
extends ../../../../../base.pug
block content
    div
        ul(class="list")
            li
                h2 Special Model (FL, GA, MD, MI, TX, VA, WA, WI)
                p Predicted Crashes=(A × B)/C
                p A=U/1+U    ,(Probability of non-zero crashes in the zero hurdle model)
                p B=Expected crash probability from the count model
                p C=1-e^(-B),(probability of non-zero crashes in the count model)
                p U=No.of years×Segment Length                             × exp(-8.175 + 0.545 LogVolume 4.841 Avg.Occupancy+0.445 LaneNumber_(6-8)).
                p B=No.of years ×Segment Length×exp⁡(-11.011+0.719 LogVolume 0.017 Diff.AvgSpeed (Upstream Target) + 1.815 FL + 4.212 WA).
            li
                h2 Generic Model
                p Predicted Crashes=(A × B)/C x CF
                p A=U/1+U    ,(Probability of non-zero crashes in the zero hurdle model)
                p B=Expected crash probability from the count model
                p C=1-e^(-B),(probability of non-zero crashes in the count model)
                p U = No. of years × Segment Length × exp(−8.175 + 0.545 LogVolume − 4.841 Avg. Occupancy + 0.445 LaneNumber6−8).
                p B = No. of years × Segment Length × exp (−11.011 + 0.719 LogVolume − 0.017 Diff. AvgSpeed (Upstream − Target)).
                ul
                    li
                        h2 Calibration Factor
                        p 𝑪𝒂𝒍𝒊𝒃𝒓𝒂𝒕𝒊𝒐𝒏 𝑭𝒂𝒄𝒕𝒐𝒓_𝒔𝒕𝒂𝒕𝒆=(∑ 〖𝑂𝑏𝑠𝑒𝑟𝑣𝑒𝑑 𝐶𝑟𝑎𝑠ℎ𝑒𝑠_𝑠𝑡𝑎𝑡𝑒 〗)/∑ 〖𝑃𝑟𝑒𝑑𝑖𝑐𝑡𝑒𝑑  𝐶𝑟𝑎𝑠ℎ𝑒𝑠_𝑠𝑡𝑎𝑡𝑒〗

        form(method="post", action="/figure", enctype="multipart/form-data")
            p
                span Select data file:
                input(type="file" name="dataFile")

            p
                span Select Model:
                select(name="model")
                    option(value="special") Special
                    option(value="generic") Generic
            p
                span Observed Crashes State:
                input(type="number" name="crashes_state" step="0.001")
            p
                button(type="submit" class="btn btn-primary")  Upload
