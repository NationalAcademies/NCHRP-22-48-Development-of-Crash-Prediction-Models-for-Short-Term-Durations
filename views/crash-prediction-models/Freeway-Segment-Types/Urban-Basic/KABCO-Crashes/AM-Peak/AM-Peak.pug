
extends ../../../../../base.pug
block content
    div
        ul(class="list")
            li
                h2 Special Model (AZ, CO, FL, GA, IL, KS, MD, MI, OR, TX, VA, WA, WI)
                p Predicted Crashes=No.of years ×Segment Length×exp⁡(-0.481+0.399 LogVolume -1.020 LogAvgSpeed+0.094 Std.Speed+0.636 LaneNumber_(6-8)+1.290 LaneNumber_(≥10)+0.476(AZ,FL,TX)+1.075(CO,VA)+1.795(GA,MI)+1.342(IL,KS))

            li
                h2 Generic Model
                p Predicted Crashes=No.of years ×Segment Length                            ×exp⁡(-0.481+0.399 LogVolume -1.020 LogAvgSpeed+0.094 Std.Speed+0.636 LaneNumber_(6-8)+1.290 LaneNumber_(≥10)) ×CF.
                ul
                    li
                        h2 Calibration Factor
                        p 𝑪𝒂𝒍𝒊𝒃𝒓𝒂𝒕𝒊𝒐𝒏 𝑭𝒂𝒄𝒕𝒐𝒓_𝒔𝒕𝒂𝒕𝒆=(∑ 〖𝑂𝑏𝑠𝑒𝑟𝑣𝑒𝑑 𝐶𝑟𝑎𝑠ℎ𝑒𝑠_𝑠𝑡𝑎𝑡𝑒 〗)/∑ 〖𝑃𝑟𝑒𝑑𝑖𝑐𝑡𝑒𝑑  𝐶𝑟𝑎𝑠ℎ𝑒𝑠_𝑠𝑡𝑎𝑡𝑒〗

        form(method="post", action="/figure", enctype="multipart/form-data")
            p
                span Select data file:
                input(type="file" name="dataFile")

            p
                span Select Model:
                select(name="model")
                    option(value="special") Special
                    option(value="generic") Generic
            p
                span Observed Crashes State:
                input(type="number" name="crashes_state" step="0.001")
            p
                button(type="submit" class="btn btn-primary")  Upload
