
extends ../../../../../base.pug
block content
    div
        ul(class="list")
            li
                h2 Special Model (AZ, CO, FL, GA, IL, KS, MD, MI, OR, TX, VA, WA, WI)
                p Predicted Crashes=No.of years ×Segment Length                             ×exp⁡(-1.182+0.303 LogVolume -0.850 LogAvgSpeed+0.087 Std.Speed+0.679 LaneNumber_(6-8)+1.253 LaneNumber_(≥10)+1.404(AZ,GA)+0.280(CO,TX)-0.434(MD,WA,WI)+0.802(MI,OR,VA)).
            li
                h2 Generic Model
                p Predicted Crashes=No.of years ×Segment Length                             ×exp⁡(-1.182+0.303 LogVolume -0.850 LogAvgSpeed+0.087 Std.Speed+0.679 LaneNumber_(6-8)+1.253 LaneNumber_(≥10))×CF.
                ul
                    li
                        h2 Calibration Factor
                        p 𝑪𝒂𝒍𝒊𝒃𝒓𝒂𝒕𝒊𝒐𝒏 𝑭𝒂𝒄𝒕𝒐𝒓_𝒔𝒕𝒂𝒕𝒆=(∑ 〖𝑂𝑏𝑠𝑒𝑟𝑣𝑒𝑑 𝐶𝑟𝑎𝑠ℎ𝑒𝑠_𝑠𝑡𝑎𝑡𝑒 〗)/∑ 〖𝑃𝑟𝑒𝑑𝑖𝑐𝑡𝑒𝑑  𝐶𝑟𝑎𝑠ℎ𝑒𝑠_𝑠𝑡𝑎𝑡𝑒〗

        form(method="post", action="/figure", enctype="multipart/form-data")
            p
                span Select data file:
                input(type="file" name="dataFile")

            p
                span Select Model:
                select(name="model")
                    option(value="special") Special
                    option(value="generic") Generic
            p
                span Observed Crashes State:
                input(type="number" name="crashes_state" step="0.001")
            p
                button(type="submit" class="btn btn-primary")  Upload
