doctype html
html
    head
        link(href='https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css' rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous")
        script(src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous")
        style
            include base.css
    body
        header(class='container-fluid header')
            h1(class='title') NCHRP 22-48: DEVELOPMENT OF CRASH PREDICTION MODELS FOR SHORT-TERM DURATIONS
            h1(class='logo') NCHRP
            h5(class='introduction') NATIONAL COOPERATIVE HIGHWAY RESEARCH PROGRAM

        main(class='main')
            nav(class='nav', id="navs")
                div !{navsHTML}
                div(class='logo-box')
                    div
                        img(src="/logo1.png")
                    div
                        img(src="/logo2.png")
                    img(src="/logo3.png" class="logo3")

            div(class='content')
                h1(class="title") #{pathWay}
                block content
        script(src="/script.js")






